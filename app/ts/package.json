    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
