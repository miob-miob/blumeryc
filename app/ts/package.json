  "main": "src/main.ts",
  "scripts": {
    "build": "rm -rf dist && ts-node build-scripts/buildClientAssets && cp .env dist/bundle/.env",
    "run:build": "node dist/bundle/index.js",
    "dev": "ts-node-dev --respawn -r dotenv/config src/index.ts",
    "kill:2020": "kill $(lsof -ti:2020)",
    "l": "yarn run lint:fix",
    "lint": "eslint \"{src}/**/*.{js,ts}\"",
    "lint:fix": "eslint \"{src}/**/*.{js,ts}\" --fix",
    "ts:check-types": "tsc",
    "ts:check-types:watch": "tsc --watch"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "node-fetch": "^2.6.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^9.3.1",
    "typed-env-parser": "^0.1.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.34",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
